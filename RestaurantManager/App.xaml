<?xml version="1.0" encoding="utf-8" ?>
<dryIoc:PrismApplication xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dryIoc="http://prismlibrary.com"
             xmlns:resources="clr-namespace:RestaurantManager.Resources;assembly=RestaurantManager"
             x:Class="RestaurantManager.App">
    <Application.Resources>
        <Color x:Key="WindowBackgroundColor">White</Color>
        <Color x:Key="WindowBackgroundColorDark">Black</Color>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <resources:Colors/>
                <resources:Fonts/>
                <resources:Styles/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</dryIoc:PrismApplication>